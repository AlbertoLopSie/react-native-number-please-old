  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/lodash.find": "^4.6.6",
    "@types/lodash.findindex": "^4.6.6",
    "@types/react": "^16.9.32",
    "@types/react-native": "^0.62.2",
    "@types/react-test-renderer": "^16.9.2",
    "microbundle": "^0.11.0",
